{
  "connection": "default",
  "collectionName": "blog_categories",
  "info": {
    "name": "Blog Category"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "blog_posts": {
      "collection": "blog-post",
      "via": "blog_categories",
      "dominant": true
    }
  }
}
